
// Soal no 1

package main

import "fmt"

func main() {
	var x, y int
	 
	fmt.Println("Masukan Nilai x:")
	fmt.Scanln(&x)

	fmt.Println("Masukan Nilai y")
	fmt.Scanln(&y)

	if y < x {
		fmt.Println("Nilai y harus lebih besar dari x")
	}

	var sum int
	for i := x; i<= y; i++ {
			sum +=i
	}

	fmt.Printf("Jumlah bilangan dari %d sampai %d adalah: %d\n", x, y, sum)
}

